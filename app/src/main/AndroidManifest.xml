<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.first.tripakey.caldecorate" >

    <!-- Include required permissions for Google Mobile Ads to run -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- This meta-data tag is required to use Google Play Services. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".decorate.curtain"
            android:label="@string/title_activity_curtain" >
        </activity>
        <activity
            android:name=".cost.cost_curtain"
            android:label="@string/title_activity_cost1" >
        </activity>
        <activity
            android:name=".decorate.WallPaper"
            android:label="@string/title_activity_wall_paper" >
        </activity>
        <activity
            android:name=".decorate.RollerBlind"
            android:label="@string/title_activity_cost2" >
        </activity>
        <activity
            android:name=".cost.costFurnitureFabrics"
            android:label="@string/title_activity_cost1_2" >
        </activity>
        <activity
            android:name=".decorate.BambooBlind"
            android:label="@string/title_activity_cost2_2" >
        </activity>
        <activity
            android:name=".decorate.VenetianBlind"
            android:label="@string/title_activity_cost2_3" >
        </activity>
        <activity
            android:name=".decorate.RoomPartition"
            android:label="@string/title_activity_cost2_4" >
        </activity>
        <activity
            android:name=".cost.costWallpaper"
            android:label="@string/title_activity_cost_wallpaper" >
        </activity>

        <!-- Include the AdActivity configChanges and theme. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name=".tabDesign"
            android:label="@string/title_activity_tab_design" >
        </activity>
        <activity
            android:name=".main.Tab1"
            android:label="@string/title_activity_tab1" >
        </activity>
        <activity
            android:name=".first_Time.firstTime"
            android:label="@string/title_activity_first_time" >
        </activity>
        <activity
            android:name=".main.Tab3"
            android:label="@string/title_activity_tab3" >
        </activity>
        <activity
            android:name=".decorate.curtains.Eyelet.Eyelet"
            android:label="@string/title_activity_eyelet" >
        </activity>
        <activity
            android:name=".decorate.curtains.Pleated.Pleated"
            android:label="@string/title_activity_pleated" >
        </activity>
        <activity
            android:name=".decorate.curtains.Roman.Roman"
            android:label="@string/title_activity_roman" >
        </activity>
    </application>

</manifest>
