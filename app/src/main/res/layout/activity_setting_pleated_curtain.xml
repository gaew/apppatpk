<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    android:layout_weight="1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#34495e">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.first.triparky.caldecorate.setting"
        android:background="#34495e"

        >


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="ปริมาณผ้าที่ต้องใช้"
                android:id="@+id/textView4"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/setD_edittext"
                android:hint="2.5 ซ.ม."
                android:textColor="#f39c12"
                android:textColorHint="#afa3a3a3" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="เท่า ของความกว้าง"/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="พับเย็บเทปเคมี"
                android:id="@+id/textView5"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setE_edittext"
                android:layout_column="0"
                android:hint="10 ซ.ม."
                android:textColorHint="#afa3a3a3"

                android:textColor="#f39c12" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="ซ.ม."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="พับเย็บผ้าใส่โซ่"
                android:id="@+id/textView6"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setF_edittext"
                android:layout_column="0"
                android:hint="10 ซ.ม."
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="ซ.ม."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="เผื่อความกว้าง ด้านละ"
                android:id="@+id/textView7"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setG_edittext"
                android:layout_column="0"
                android:hint="15 ซ.ม."
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="ซ.ม."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="เผื่อชายผ้าด้านล่าง"
                android:id="@+id/textView8"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setH_edittext"
                android:layout_column="0"
                android:hint="30 ซ.ม."
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="ซ.ม."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="เผื่อจำนวนผ้า (%)"
                android:id="@+id/textView9"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">

            <Spinner
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/setI_spin"
                android:layout_column="0"
                android:layout_weight="1" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="%."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="สายรวบม่าน สำหรับ  2  เส้น"
                android:id="@+id/textView"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">

            <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setJ_edittext"
                android:layout_column="0"
                android:hint="50 ซ.ม."
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="#fff"
                android:text="ซ.ม."/>
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="บันทึกการเปลี่ยนแปลง"
                android:id="@+id/changeSet_button"
                android:layout_column="0"
                android:enabled="true"
                android:textColor="#ffffffff"
                android:layout_weight="1"
                android:background="#27ae60"
                android:textSize="20dp" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="ใช้ค่ามาตรฐาน"
                android:id="@+id/noChSet_button"
                android:layout_column="0"
                android:textColor="#ffffffff"
                android:layout_weight="1"
                android:background="#3498db"
                android:layout_marginTop="5dp"
                android:textSize="20dp" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>
    </TableLayout>
    </ScrollView>
</RelativeLayout>
